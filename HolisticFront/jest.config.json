{
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "moduleNameMapper": {
    "\\.(css|less|scss|sass)$": "identity-obj-proxy",
    "\\.(svg)$": "<rootDir>/tests/__mocks__/images/login.svg"
  },
  "testEnvironment": "jsdom",
  "transformIgnorePatterns": [
    "/node_modules/",
    "\\.pnp\\.[^\\/]+$",
    "\\.css$"
  ]
}
